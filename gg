Id	int	Unchecked
Codigo	varchar(15)	Checked
Nombre	varchar(300)	Checked
ArchivoPdf	varchar(MAX)	Checked
UsuarioRegistro	varchar(50)	Checked
FechaRegistro	datetime	Checked
UsuarioModifico	varchar(50)	Checked
FechaModifico	datetime	Checked

public int Id { get; set; }
public string Codigo { get; set; }
public string Nombre { get; set; }
public string ArchivoPdf { get; set; }
public string UsuarioRegistro { get; set; }
public DateTime FechaRegistro { get; set; }
public string UsuarioModifico { get; set; }
public DateTime FechaModifico { get; set; }

